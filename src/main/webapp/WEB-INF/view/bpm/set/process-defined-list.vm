#parse("././globle.vm" )
<html>
<head>
<title>表单列表</title>
	<script type="text/javascript" src="$cxt/static/js/select2.min.js"></script>
	<script type="text/javascript" src="$cxt/static/js/jquery.dataTables.js?20170216"></script>
	<script type="text/javascript" src="$cxt/static/js/DT_bootstrap.js"></script>
	<script type="text/javascript" src="$cxt/static/js/jquery.validate.min.js"></script>
	<script type="text/javascript" src="$cxt/static/js/jquery.form.min.js"></script>
    <script type="text/javascript" src="$cxt/static/js/module/process-set.js?20170317"></script>
</head>
<body>
               <!-- BEGIN PAGE HEADER-->

				<div class="row-fluid">
					<div class="span12">
					</div>
				</div>

				<!-- END PAGE HEADER-->

				<!-- BEGIN PAGE CONTENT-->

				<div class="row-fluid">

					<div class="span12">

						<!-- BEGIN EXAMPLE TABLE PORTLET-->

						<div class="portlet box purple">

							<div class="portlet-title">

								<div class="caption"><i class="icon-globe">流程定义列表</i></div>
								
								<div class="actions">
								    <a href="#" id="" 1data-toggle="deploy" class="btn red"><i class="icon-plus"></i> 部署</a>

									<a href="#" id="mySubmit" class="btn green"><i class="icon-plus"></i> 查询</a>

								</div>

							</div>
                           
							<div class="portlet-body form">
							       <!-- BEGIN FORM-->
							       <form action="#" name="searchform" id="searchform" class="form-horizontal ">
							                   <div class="row-fluid">
													<div class="span6 ">
														<div class="control-group">
															<label class="control-label" for="formName">名称</label>

															<div class="controls">

																<input type="text" name="userName" id="userName" value="" class="m-wrap" placeholder="">

															</div>

														</div>

													</div>

													<!--/span-->

													<div class="span6 ">

														<div class="control-group">

															<label class="control-label" for="lastName">编号</label>

															<div class="controls">

																<input type="text" name="formId" id="userId" value="" class="m-wrap" placeholder="">

															</div>

														</div>

													</div>

													<!--/span-->
			                                    
												</div>
											
                                            </form>                  
                                        <!-- BEGIN end-->
								<table class="table table-striped table-bordered table-hover" id="transactions">

									<thead>

										<tr>
											<th><input type="checkbox" class="group-checkable" data-set="#transactions .checkboxes" /></th>
											<th>编号</th>
											<th>名称</th>
											<th>描述</th>
											<th>创建日期</th>
											<th>操作</th>
										</tr>

									</thead>

									<tbody>

										<tr class="odd gradeX">
											<td><input type="checkbox" class="checkboxes" value="1" /></td>
											<td></td>
											<td></td>
											<td></td>
											<td></td>
											<td></td>
										</tr>

									</tbody>

								</table>

							</div>

						</div>

						<!-- END EXAMPLE TABLE PORTLET-->

					</div>

				</div>

				<div class="row-fluid">
				</div>
				
				<!-- END PAGE CONTENT-->
				
				
<div id="stack1" class="modal hide fade" tabindex="-1" data-focus-on="input:first">

									<div class="modal-header">

										<button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>

										<h3>部署</h3>

									</div>
                                    <!-- modal-body begin-->
									<div class="modal-body">
										  <div class="form">

											<!-- BEGIN FORM-->

											<form action="#" name="myform" id="myform" class="form-horizontal" encType="multipart/form-data">

												<div class="control-group">

													<label class="control-label">流程定义文件</label>

													<div class="controls">

														<input type="file" class="file" name="file" />

													</div>

												</div>

											</form>

											<!-- END FORM-->  

										</div>
									</div>
									<!-- modal-body begin-->

									<div class="modal-footer">
										<button type="button" data-dismiss="modal" class="btn">Close</button>
										<button type="button" data-dismiss="confirm" class="btn red">Ok</button>
									</div>

								</div>

</body>
</html>
