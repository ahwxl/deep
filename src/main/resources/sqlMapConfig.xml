		<plugin interceptor="com.bplow.deep.base.pagination.PageResultPlus"></plugin>
	</plugins>



	<mappers>
		<mapper resource="mapper/AutoAppInfoMapper.xml" />
		<mapper resource="mapper/BpmServiceMapper.xml" />
	</mappers>


</configuration>